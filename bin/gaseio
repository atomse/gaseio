#!/bin/bash


filename=$1
format=$2

seg1=${filename:0:2};
seg2=${filename:2:2};
dirname=$seg1/$seg2

if [ ! -e $filename ]; then
    if [ -e $dirname/$filename ]; then
        filename=$dirname/$filename
    else
        echo $filename not exist
        exit 1
    fi
fi




if [ $format ]; then
	python -c "import json_tricks, gaseio; x = gaseio.read(\"$filename\", format=\"$format\", index=':', force_gase=True); print(json_tricks.dumps(x, indent=4))"
else
	python -c "import json_tricks, gaseio; x = gaseio.read(\"$filename\", index=':', force_gase=True); print(json_tricks.dumps(x, indent=4))"
fi
